[MD]
steps                   5000
stepsize                   2.0
temperature              300.0
bath_coupling             10
integrator		invalid-integrator
separate_scaling		on
thermostat		invalid-thermostat
shake_all_hydrogens           on
lrf                       on

invalid-boundary

[cut-offs]
invalid-qatom
invalid-lrf

[intervals]
non_bond                  25
output                    50

[files]
topology                  lig_w.top
fep                       lig_w.fep
final                     eq4.re
restart                   eq3.re

[sequence_restraints]
1 46 0.5 0 1

