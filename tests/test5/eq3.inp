[MD]
steps                   1000
stepsize                   2.0
temperature              150.0
bath_coupling             10
separate_scaling		on

shake_hydrogens           on
lrf                       on

[PBC]
constant_pressure       on
max_volume_displ        5
pressure_seed           1337
pressure                1.01
put_solvent_back_in_box on
put_solute_back_in_box  off

[cut-offs]
q_atom			24
lrf			24

[intervals]
non_bond                  25
output                    50

[files]
topology                  lig_w.top
fep                       lig_w.fep
final                     eq3.re
restart                   eq2.re

[sequence_restraints]
1 46 0.5 0 1

